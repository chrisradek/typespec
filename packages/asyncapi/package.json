{
  "name": "@typespec/asyncapi",
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://typespec.io",
  "readme": "https://github.com/microsoft/typespec/blob/main/packages/asyncapi/README.md",
  "scripts": {
    "build": "pnpm gen-extern-signature && alloy build",
    "clean": "rimraf ./dist",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "watch": "alloy build --watch",
    "gen-extern-signature": "tspd --enable-experimental gen-extern-signature .",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest -w",
    "test:regen": "cross-env RECORD=true vitest run",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=default",
    "format": "prettier . --write",
    "build:test": "tsc -p tsconfig.test.json",
    "regen-docs": "tspd doc .  --enable-experimental  --output-dir ../../website/src/content/docs/docs/emitters/asyncapi/reference --skip-js"
  },
  "exports": {
    ".": {
      "typespec": "./lib/main.tsp",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "keywords": [
    "typespec",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/microsoft/typespec/issues"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "description": "TypeSpec library for emitting Http Client libraries for JavaScript/TypeScript",
  "peerDependencies": {
    "@typespec/compiler": "workspace:^",
    "@typespec/events": "workspace:^"
  },
  "dependencies": {
    "@alloy-js/core": "^0.15.0",
    "@alloy-js/json": "^0.15.0",
    "@alloy-js/typescript": "^0.15.0",
    "@typespec/emitter-framework": "workspace:^",
    "@typespec/http-client": "workspace:^",
    "prettier": "~3.5.3"
  },
  "devDependencies": {
    "@alloy-js/cli": "^0.15.0",
    "@alloy-js/rollup-plugin": "^0.1.0",
    "@types/yargs": "~17.0.33",
    "@typespec/events": "workspace:^",
    "@typespec/tspd": "workspace:^",
    "@vitest/ui": "^3.1.2",
    "change-case": "~5.4.4",
    "concurrently": "^9.1.2",
    "cross-env": "~7.0.3",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "globby": "~14.1.0",
    "inquirer": "^12.5.0",
    "ora": "^8.1.1",
    "p-limit": "^6.2.0",
    "picocolors": "~1.1.1",
    "typescript": "~5.8.2",
    "uri-template": "^2.0.0",
    "vitest": "^3.1.2",
    "yargs": "~17.7.2"
  }
}
